{
  "name": "chatbot-client",
  "version": "0.0.0",
  "main": "server.js",
  "description": "Chatbot Client",
  "author": "Aditio Pangestu",
  "license": "MIT",
  "keywords": [
    "chatbot",
    "next.js",
    "react",
    "boilerplate",
    "apollo",
    "ran",
    "run",
    "toolkit"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "start:multicore": "cross-env NODE_ENV=production pm2 start server.js -i max --attach",
    "build": "npm run clean-cache && next build",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "dev": "npm run lint && npm run clean-cache && node server.js",
    "lint": "eslint --fix --ext .js pages components libraries server helper_scripts",
    "lint:watch": "esw -w --fix components libraries server",
    "analyze": "cross-env ANALYZE_BUILD=true npm run build",
    "setup": "node ./helper_scripts/CL_commands/setup.js && npm run setup:after",
    "setup:clean": "cross-env CLEANSETUP=true node ./helper_scripts/CL_commands/setup.js && npm run setup:after",
    "setup:after": "npm run build",
    "create:page": "node ./helper_scripts/CL_commands/create_page.js",
    "create:route": "node ./helper_scripts/CL_commands/create_route.js",
    "create:container": "node ./helper_scripts/CL_commands/create_container.js",
    "create:component": "node ./helper_scripts/CL_commands/create_component.js",
    "lint-staged": "lint-staged",
    "heroku-postbuild": "npm run build",
    "prestart": "npm run build",
    "release": "release",
    "build:static_export": "cross-env STATIC_EXPORT=true npm run build && cross-env STATIC_EXPORT=true next export",
    "zip-for-beanstalk": "npm prune --production && zip -qr ran-boilerplate.zip . -x \"*.zip\""
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "0.3.1",
    "apollo-client-preset": "1.0.8",
    "babel-plugin-import-graphql": "2.7.0",
    "chalk": "2.4.2",
    "color": "3.1.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "5.2.0",
    "dotenv": "7.0.0",
    "dotenv-webpack": "1.7.0",
    "downshift": "^3.2.7",
    "draft-js": "^0.10.5",
    "express": "4.16.4",
    "final-form": "^4.12.0",
    "graphql": "^14.2.1",
    "helmet": "3.16.0",
    "ip": "1.1.5",
    "isomorphic-fetch": "2.2.1",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.11",
    "lru-cache": "5.1.1",
    "material-ui-chip-input": "^1.0.0-beta.15",
    "material-ui-flat-pagination": "^3.2.0",
    "moment": "2.24.0",
    "next": "7.0.3",
    "next-cookies": "Sly777/next-cookies#master",
    "next-nprogress": "^1.4.0",
    "next-routes": "1.4.2",
    "notistack": "^0.8.2",
    "offline-plugin": "5.0.6",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-apollo": "2.5.3",
    "react-apollo-network-status": "^1.1.0",
    "react-clipboard.js": "^2.0.7",
    "react-dom": "16.8.6",
    "react-final-form-hooks": "^1.0.1",
    "react-jss": "^8.6.1",
    "react-redux": "6.0.1",
    "react-scrollbars-custom": "^4.0.0-alpha.12",
    "read-excel-file": "^3.0.0",
    "redux": "4.0.1",
    "redux-thunk": "^2.3.0",
    "rimraf": "2.6.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-flow-strip-types": "7.4.0",
    "babel-eslint": "10.0.1",
    "cli-clear": "1.0.4",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.4.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-watch": "5.0.1",
    "figlet": "1.2.1",
    "flow-bin": "0.95.1",
    "handlebars": "^4.1.2",
    "husky": "1.3.1",
    "inquirer": "6.2.2",
    "lint-staged": "8.1.5",
    "ngrok": "3.1.1",
    "pre-commit": "1.2.2",
    "prettier": "1.16.4",
    "replace-in-file": "3.4.4",
    "shelljs": "0.8.3",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/ran"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
